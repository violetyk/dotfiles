[user]
  name = kagaya
  #email = yuhei.kagaya@gmail.com
  email = yuhei.kagaya@nanapi.co.jp

[color]
  ui = auto
[http]
  sslVerify = false
[alias]
  st = status
  br = "branch -a"
  co = "checkout"
  unstage = "reset HEAD --"
  addu = "add --update"
  adda = "add --all"

[core]
  #editor = vim -c \"set fenc=utf-8\"
  editor = /usr/local/vim-7.3/bin/vim -c \"set fenc=utf-8\"
  excludesfile = ~/.gitignore
